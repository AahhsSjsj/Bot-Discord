#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir//bin/bash" ]; then
  "$basedir//bin/bash" -e "$basedir/../keepalive/keepalive.sh" "$@"
  ret=$?
else 
  /bin/bash -e "$basedir/../keepalive/keepalive.sh" "$@"
  ret=$?
fi
exit $ret
